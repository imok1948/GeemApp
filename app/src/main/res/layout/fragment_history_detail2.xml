<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".temp.ui.ui.browse2.history2.history.FragmentHistoryDetail">

  <!--
  <androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

-->
  <TextView
    android:id="@+id/history_detail_historyId"
    android:layout_width="360dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="20dp"
    android:paddingLeft="12dp"
    android:paddingTop="6dp"
    android:paddingEnd="6dp"
    android:paddingRight="12dp"
    android:text="TextView"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent" />

  <androidx.constraintlayout.widget.Barrier
    android:layout_width="match_parent"
    android:layout_height="10dp"
    app:barrierDirection="top"
    tools:layout_editor_absoluteX="101dp"
    tools:layout_editor_absoluteY="55dp" />

  <TextView
    android:id="@+id/history_detail_post_title"
    android:layout_width="360dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    android:paddingLeft="12dp"
    android:paddingTop="6dp"
    android:paddingEnd="6dp"
    android:paddingRight="12dp"
    android:text="Swami Vivekananda's Karmayoga"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.509"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_detail_historyId" />


  <!--
  <ImageView
    android:id="@+id/history_detail_post_image"
    android:layout_width="360dp"
    android:layout_height="42dp"
    android:layout_marginTop="16dp"
    android:padding="6dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_detail_post_title"
    tools:src="@tools:sample/avatars" />
-->

  <androidx.viewpager.widget.ViewPager
    android:id="@+id/history_detail_view_pager_images"
    android:layout_width="331dp"
    android:layout_height="213dp"
    android:layout_marginStart="16dp"
    android:layout_marginLeft="16dp"
    android:layout_marginTop="48dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_detail_post_title" />

  <TextView
    android:id="@+id/history_post_description"
    android:layout_width="360dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="24dp"
    android:paddingLeft="12dp"
    android:paddingTop="6dp"
    android:paddingEnd="6dp"
    android:paddingRight="12dp"
    android:text="This is a very good book, with very good contents, condition is also very good"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_detail_view_pager_images" />

  <View
    android:id="@+id/divider4"
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:layout_marginTop="12dp"
    android:background="?android:attr/listDivider"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_post_description" />

  <TextView
    android:id="@+id/history_detail_post_owner"
    android:layout_width="360dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="4dp"
    android:paddingLeft="12dp"
    android:paddingTop="6dp"
    android:paddingEnd="6dp"
    android:paddingRight="12dp"
    android:text="From Rahul Meena"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/divider4" />

  <TextView
    android:id="@+id/history_detail_item_posting_time"
    android:layout_width="360dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="4dp"
    android:paddingLeft="12dp"
    android:paddingTop="6dp"
    android:paddingEnd="6dp"
    android:paddingRight="12dp"
    android:text="On Tue, 26 Jan"
    android:textStyle="bold"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.456"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_detail_post_owner" />

  <TextView
    android:id="@+id/history_detail_post_address"
    android:layout_width="360dp"
    android:layout_height="wrap_content"
    android:layout_marginTop="4dp"
    android:paddingLeft="12dp"
    android:paddingTop="6dp"
    android:paddingEnd="6dp"
    android:paddingRight="12dp"
    android:text="A,37/1, Midc Indl Area, Bhnd Bhangar Kata, Taloja, Navi Mumbai"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_detail_item_posting_time" />

  <com.google.android.gms.maps.MapView
    android:id="@+id/mapView"
    android:layout_width="338dp"
    android:layout_height="177dp"
    android:layout_marginTop="8dp"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintHorizontal_bias="0.727"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/history_detail_post_address" />


</androidx.constraintlayout.widget.ConstraintLayout>